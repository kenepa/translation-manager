{
  "name": "kenepa/translation-manager",
  "description": "Manage your application's translation strings in Filament.",
  "keywords": [
    "Kenepa",
    "laravel",
    "filament",
    "localization",
    "translation"
  ],
  "homepage": "https://github.com/kenape/translation-manager",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kenepa\\TranslationManager\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "filament/filament": "^2.17",
    "filament/notifications": "^2.17",
    "spatie/laravel-package-tools": "^1.10",
    "spatie/laravel-translation-loader": "^2.7",
    "stijnvanouplines/blade-country-flags": "^1.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kenepa\\TranslationManager\\TranslationManagerProvider"
      ]
    }
  },
  "prefer-stable": true,
  "require-dev": {
    "tightenco/duster": "^2.0"
  }
}
